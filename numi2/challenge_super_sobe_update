/* assign promo code C9_T1_Task1 Bonus ***********************************************************/

-- @wbtag C18_C11_ assign promo codes all
insert into promo_code_users_sb  (user_id, attribute_name, data_type, campaign_stream_steps_id, list_users_id)
select t1.user_id, css.attribute_name, css.data_type, css.id as campaign_stream_steps_id, t1.list_users_id 
from tmp_success_summary t1
inner join campaign_stream_steps_sobe css on t1.campaign_stream_steps_id = css.day_in and rule_type = 'promo_code';

/*  step 2 - update user promo codes *************************************************************/
update promo_code_users_sb aa
set promo_code = tt.promo_code
from (
    select ap.user_id, ap.attribute_name, pc.promo_code from (select user_id, attribute_name, campaign_stream_steps_id, list_users_id, row_number() over (partition by attribute_name order by  user_id) as row_num from promo_code_users_sb
    where  promo_code is null) ap
    inner join (select id, promo_code, attribute_name, row_number() over (partition by attribute_name order by id) as row_num from promo_codes_sb pc
    where pc.attribute_name in (select distinct(attribute_name) from promo_code_users_sb) 
    and pc.user_id is null
) pc on ap.row_num = pc.row_num and ap.attribute_name = pc.attribute_name) tt
where aa.user_id = tt.user_id and aa.attribute_name = tt.attribute_name;

update promo_codes_sb pc
set user_id = pcu.user_id
from promo_code_users_sb pcu
where pcu.attribute_name = pc.attribute_name and pcu.promo_code = pc.promo_code 
and pc.user_id is null;

/* step 3 - send appboy user events ********************/

insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select user_id, attribute_name, data_type, promo_code as data_value, campaign_stream_steps_id, list_users_id from promo_code_users_sb where date_sent is null
except  select user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id  from appboy_user_events_sb ;

update promo_code_users_sb
set date_sent = now()
where date_sent is null;

-- @wbtag C18_C11 success end date
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select t1.user_id, css2.attribute_name, css2.data_type, to_char(lu.list_start_date::date + day_end + 6 * INTERVAL '1 day', 'MM /DD /YYYY') as data_value, css2.id as campaign_stream_steps_id, t1.list_users_id  from (
select tss.user_id, tss.attribute_name, tss.data_type, tss.data_value, tss.campaign_stream_steps_id, tss.list_users_id, css.id, css.campaign_id from tmp_success_summary tss
inner join campaign_stream_steps_sobe css on tss.campaign_stream_steps_id = css.id) t1
inner join campaign_stream_steps_sobe css2 on t1.campaign_id = css2.campaign_id and css2.rule_type = 'promo_code_end_date'
inner join list_users_c18_sobe lu on lu.user_id = t1.user_id;

/* @wbtag C18_C11 insert appboy_user_events from tmp_success_summary  *************************************************************/

insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select tss.user_id, tss.attribute_name, css.data_type, css.data_value, tss.campaign_stream_steps_id, tss.list_users_id from tmp_success_summary tss
inner join campaign_stream_steps_sobe css on tss.campaign_stream_steps_id = css.id;

/* @wbtag C18_C11 expiration 7 days after challenge ends *************************************************************/
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select tt.user_id, tt.attribute_name, tt.data_type, tt.data_value, tt.campaign_stream_steps_id, tt.list_users_id from appboy_user_events_sb aue 
inner join (
select lu.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id, css.day_in
from list_users_c18_sobe lu 
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id = 4 and rule_type = 'result_status_success_expiration' -- and campaign_sequence = 1
where  lu.campaign_id = 1
) tt on aue.user_id = tt.user_id and aue.campaign_stream_steps_id = tt.day_in
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id where  css.campaign_id = 4 and rule_type = 'result_status_success_expiration';
 --where campaign_stream_steps_id = 199;

/* @wbtag C18_C10 expiration 7 days after challenge ends *************************************************************/
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select tt.user_id, tt.attribute_name, tt.data_type, tt.data_value, tt.campaign_stream_steps_id, tt.list_users_id from appboy_user_events_sb aue 
inner join (
select lu.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id, css.day_in
from list_users_c18_sobe lu 
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id = 3 and rule_type = 'result_status_success_expiration' -- and campaign_sequence = 1
where  lu.campaign_id = 1
) tt on aue.user_id = tt.user_id and aue.campaign_stream_steps_id = tt.day_in
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id where  css.campaign_id = 3 and rule_type = 'result_status_success_expiration';
 --where campaign_stream_steps_id = 199;

-- @wbtag C18_C12_BONUS_SUCCESS
-- @wbtag C18_C12_SUCCESS temp table
drop table if exists tmp_sobe_challenge_12_success;
create temp table tmp_sobe_challenge_12_success  (user_id integer, attribute_name varchar(255), data_type varchar(255), data_value varchar(255), campaign_stream_steps_id integer, list_users_id integer);

-- @wbtag C18_C12 bonus 1 success water goal = 1 day
insert into tmp_sobe_challenge_12_success (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id,  css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from tmp_sobe_challenge_12 usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (5) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 1
and usw.campaign_stream_steps_id = css.day_in
where usw.cnt >= 1  -- 10
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 1 and css.campaign_id in  (5);

-- @wbtag C18_C12 bonus 2 success water goal = 7 day
insert into tmp_sobe_challenge_12_success (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id,  css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from tmp_sobe_challenge_12 usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (5) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 2
and usw.campaign_stream_steps_id = css.day_in
where usw.cnt >= 7  -- 10
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 2 and css.campaign_id in  (5);

-- @wbtag C18_C12 tier 1-4 success 8 oz goal 5,10,15,20
insert into tmp_sobe_challenge_12_success (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id,  css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from tmp_sobe_challenge_12 usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (5) and rule_type = 'result_status_success' and campaign_stream_sequence in (8) and task_number = 1
and usw.campaign_stream_steps_id = css.day_in
where usw.cnt >= 5  -- 10
  union
select usw.user_id,  css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from tmp_sobe_challenge_12 usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (5) and rule_type = 'result_status_success' and campaign_stream_sequence in (8) and task_number = 2
and usw.campaign_stream_steps_id = css.day_in
where usw.cnt >= 10 
  union
select usw.user_id,  css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from tmp_sobe_challenge_12 usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (5) and rule_type = 'result_status_success' and campaign_stream_sequence in (8) and task_number = 3
and usw.campaign_stream_steps_id = css.day_in
where usw.cnt >= 15 
  union
select usw.user_id,  css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from tmp_sobe_challenge_12 usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (5) and rule_type = 'result_status_success' and campaign_stream_sequence in (8) and task_number = 4
and usw.campaign_stream_steps_id = css.day_in
where usw.cnt >= 20
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'result_status_success' and campaign_stream_sequence in (8) and task_number in (1,2,3,4) and css.campaign_id in  (5);


-- @wbtag C18_C12 bonus super status log >= 640 oz
-- dont insert into tmp_sobe_challenge_12, no promo code is being sent
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id,  css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from tmp_sobe_challenge_12 usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (5) and rule_type = 'result_status_success' and campaign_stream_sequence in (5) and task_number = 1
and usw.campaign_stream_steps_id = css.day_in
where usw.cnt >= 640  -- 10
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'result_status_success' and campaign_stream_sequence in (5) and task_number = 1 and css.campaign_id in  (5);

-- @wbtag C18_C12_ assign promo codes all wio
insert into promo_code_users_sb  (user_id, attribute_name, data_type, campaign_stream_steps_id, list_users_id)

select t1.user_id, css.attribute_name, css.data_type, css.id as campaign_stream_steps_id, t1.list_users_id 
from tmp_sobe_challenge_12_success t1
inner join campaign_stream_steps_sobe css on t1.campaign_stream_steps_id = css.day_in and css.rule_type = 'promo_code';

/*  step 2 - update user promo codes *************************************************************/
update promo_code_users_sb aa
set promo_code = tt.promo_code
--select * 
from (
    select ap.user_id, ap.attribute_name, pc.promo_code from (select user_id, attribute_name, campaign_stream_steps_id, list_users_id, row_number() over (partition by attribute_name order by  user_id) as row_num from promo_code_users_sb
    where  promo_code is null) ap
    inner join (select id, promo_code, attribute_name, row_number() over (partition by attribute_name order by id) as row_num from promo_codes_sb pc
    where pc.attribute_name in (select distinct(attribute_name) from promo_code_users_sb) 
    and pc.user_id is null
) pc on ap.row_num = pc.row_num and ap.attribute_name = pc.attribute_name) tt
where aa.user_id = tt.user_id and aa.attribute_name = tt.attribute_name;

update promo_codes_sb pc
set user_id = pcu.user_id
from promo_code_users_sb pcu
where pcu.attribute_name = pc.attribute_name and pcu.promo_code = pc.promo_code 
and pc.user_id is null;

/* step 3 - send appboy user events ********************/

insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select user_id, attribute_name, data_type, promo_code as data_value, campaign_stream_steps_id, list_users_id from promo_code_users_sb where date_sent is null
except  select user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id  from appboy_user_events_sb ;

update promo_code_users_sb
set date_sent = now()
where date_sent is null;

-- @wbtag C18_C12 success end date
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select t1.user_id, css2.attribute_name, css2.data_type, to_char(lu.list_start_date::date + day_end + 6 * INTERVAL '1 day', 'MM /DD /YYYY') as data_value, css2.id as campaign_stream_steps_id, t1.list_users_id  from (
select tss.user_id, tss.attribute_name, tss.data_type, tss.data_value, tss.campaign_stream_steps_id, tss.list_users_id, css.id, css.campaign_id from tmp_sobe_challenge_12_success tss
inner join campaign_stream_steps_sobe css on tss.campaign_stream_steps_id = css.id) t1
inner join campaign_stream_steps_sobe css2 on t1.campaign_id = css2.campaign_id and css2.rule_type = 'promo_code_end_date'
inner join list_users_c18_sobe lu on lu.user_id = t1.user_id;

/* @wbtag C18_C12 insert appboy_user_events from tmp_success_summary  *************************************************************/

insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select tss.user_id, tss.attribute_name, css.data_type, css.data_value, tss.campaign_stream_steps_id, tss.list_users_id from tmp_sobe_challenge_12_success tss
inner join campaign_stream_steps_sobe css on tss.campaign_stream_steps_id = css.id;

/* @wbtag C18_C12 expiration 7 days after challenge ends *************************************************************/
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select tt.user_id, tt.attribute_name, tt.data_type, tt.data_value, tt.campaign_stream_steps_id, tt.list_users_id from appboy_user_events_sb aue 
inner join (
select lu.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id, css.day_in
from list_users_c18_sobe lu 
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id = 5 and rule_type = 'result_status_success_expiration' -- and campaign_sequence = 1
where  lu.campaign_id = 1 order by user_id desc
) tt on aue.user_id = tt.user_id and aue.campaign_stream_steps_id = tt.day_in
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id where  css.campaign_id = 5 and rule_type = 'result_status_success_expiration';

-- @wbtag C18_C13_BONUS_SUCCESS
-- @wbtag C18_C13_SUCCESS temp table
drop table if exists tmp_sobe_challenge_13_success;
create temp table tmp_sobe_challenge_13_success  (user_id integer, attribute_name varchar(255), data_type varchar(255), data_value varchar(255), campaign_stream_steps_id integer, list_users_id integer);

-- @wbtag C18_C13 day count
insert into tmp_sobe_challenge_13_success (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id, css.attribute_name, css.data_type, cast(sum(usw.days_logged) as varchar) as data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from user_stats_weekly_mon_sobe usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'task_count' and campaign_sequence in (13) and task_number = 1
where usw.first_day_of_week between (lu.list_start_date + interval '1 day' * css.day_start )::date and (lu.list_start_date + interval '1 day' * css.day_end  - interval '1 day' )::date 
group by usw.user_id, css.attribute_name, css.id, css.data_type,lu.list_id, lu.list_start_date
having sum(usw.days_logged) > 0;
--except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
--inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'task_count' and campaign_sequence in (13) and task_number = 1 and css.campaign_id in (6); 

-- @wbtag C18_C13 food count
insert into tmp_sobe_challenge_13_success (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select usw.user_id, css.attribute_name, css.data_type, cast(sum(usw.consumed_total_items_logged) as varchar) as data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from user_stats_weekly_mon_sobe usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'task_count' and campaign_sequence in (13) and task_number = 2
where usw.first_day_of_week between (lu.list_start_date + interval '1 day' * css.day_start )::date and (lu.list_start_date + interval '1 day' * css.day_end  - interval '1 day' )::date 
group by usw.user_id, css.attribute_name, css.id, css.data_type,lu.list_id, lu.list_start_date
having sum(usw.consumed_total_items_logged) > 0;
--except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
--inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'task_count' and campaign_sequence in  (13) and task_number = 2 and css.campaign_id in (6); 

-- @wbtag C18_C13 activity count
insert into tmp_sobe_challenge_13_success (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id, css.attribute_name, css.data_type, cast(sum(usw.active_days_entries) as varchar) as data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from user_stats_weekly_mon_sobe usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'task_count' and campaign_sequence in (13) and task_number = 3
where usw.first_day_of_week between (lu.list_start_date + interval '1 day' * css.day_start )::date and (lu.list_start_date + interval '1 day' * css.day_end  - interval '1 day' )::date 
group by usw.user_id, css.attribute_name, css.id, css.data_type,lu.list_id, lu.list_start_date
having sum(usw.active_days_entries) > 0;
--except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
--inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'task_count' and campaign_sequence in (13) and task_number = 3 and css.campaign_id in (6); 

-- @wbtag C18_C13 water count
insert into tmp_sobe_challenge_13_success (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id, css.attribute_name, css.data_type, cast(sum(usw.water_days_entries) as varchar) as data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from user_stats_weekly_mon_sobe usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'task_count' and campaign_sequence in (13) and task_number = 4
where usw.first_day_of_week between (lu.list_start_date + interval '1 day' * css.day_start )::date and (lu.list_start_date + interval '1 day' * css.day_end  - interval '1 day' )::date 
group by usw.user_id, css.attribute_name, css.id, css.data_type,lu.list_id, lu.list_start_date
having sum(usw.water_days_entries) > 0;
--except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
--inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'task_count' and campaign_sequence in (13) and task_number = 4 and css.campaign_id in (6); 

insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select tss.user_id, tss.attribute_name, css.data_type, tss.data_value, tss.campaign_stream_steps_id, tss.list_users_id from tmp_sobe_challenge_13_success tss
inner join campaign_stream_steps_sobe css on tss.campaign_stream_steps_id = css.id
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'task_count' and campaign_sequence in (13) and task_number in (1,2,3,4) and css.campaign_id in (6);

--select * from tmp_sobe_challenge_13_success tss where user_id = 8988

-- @wbtag C18_C13_BONUS_1_SUCCESS Log activity >= 10 days
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.user_id as list_users_id
from tmp_sobe_challenge_13_success usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 1
and css.day_in = usw.campaign_stream_steps_id
where cast(usw.data_value as integer) >= 10
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id where css.campaign_id in (6) and css.rule_type = 'result_status_success' and css.campaign_stream_sequence in (6) and css.task_number = 1;

-- @wbtag C18_C13_BONUS_1_SUCCESS Log water >= 20 days
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.user_id as list_users_id
from tmp_sobe_challenge_13_success usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 2
and css.day_in = usw.campaign_stream_steps_id
where cast(usw.data_value as integer) >= 20
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id where css.campaign_id in (6) and css.rule_type = 'result_status_success' and css.campaign_stream_sequence in (6) and css.task_number = 2;

-- @wbtag C18_C13_BONUS_SUPER STATUS Log food count >= 200
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.user_id as list_users_id
from tmp_sobe_challenge_13_success usw
inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (5) and task_number = 1
and css.day_in = usw.campaign_stream_steps_id
where cast(usw.data_value as integer) >= 200
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id where css.campaign_id in (6) and css.rule_type = 'result_status_success' and css.campaign_stream_sequence in (5) and css.task_number = 1;


-- @wbtag C18_C13_ assign promo codes all
insert into promo_code_users_sb  (user_id, attribute_name, data_type, campaign_stream_steps_id, list_users_id)
select zz.user_id, zz.attribute_name, zz.data_type, zz. campaign_stream_steps_id, zz.list_users_id 
from (
   select tt.user_id, css.attribute_name, css.data_type,  css.id as campaign_stream_steps_id, tt.list_users_id from (
     select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.list_users_id
    from tmp_sobe_challenge_13_success usw
    inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
    inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 1
    and css.day_in = usw.campaign_stream_steps_id
    where cast(usw.data_value as integer) >= 10
    union
    select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.list_users_id as list_users_id
    from tmp_sobe_challenge_13_success usw
    inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
    inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 2
    and css.day_in = usw.campaign_stream_steps_id
    where cast(usw.data_value as integer) >= 20
    union
    select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.list_users_id as list_users_id
    from tmp_sobe_challenge_13_success usw
    inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
    inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (5) and task_number = 1
    and css.day_in = usw.campaign_stream_steps_id
    where cast(usw.data_value as integer) >= 200
    ) tt
    inner join campaign_stream_steps_sobe css on tt.campaign_stream_steps_id = css.day_in and rule_type = 'promo_code'
    ) zz
 except select user_id, attribute_name, data_type, campaign_stream_steps_id, list_users_id from promo_code_users_sb;

/*  step 2 - update user promo codes *************************************************************/
update promo_code_users_sb aa
set promo_code = tt.promo_code
from (
    select ap.user_id, ap.attribute_name, pc.promo_code from (select user_id, attribute_name, campaign_stream_steps_id, list_users_id, row_number() over (partition by attribute_name order by  user_id) as row_num from promo_code_users_sb
    where  promo_code is null) ap
    inner join (select id, promo_code, attribute_name, row_number() over (partition by attribute_name order by id) as row_num from promo_codes_sb pc
    where pc.attribute_name in (select distinct(attribute_name) from promo_code_users_sb) 
    and pc.user_id is null
) pc on ap.row_num = pc.row_num and ap.attribute_name = pc.attribute_name) tt
where aa.user_id = tt.user_id and aa.attribute_name = tt.attribute_name;

update promo_codes_sb pc
set user_id = pcu.user_id
from promo_code_users_sb pcu
where pcu.attribute_name = pc.attribute_name and pcu.promo_code = pc.promo_code 
and pc.user_id is null;

/* step 3 - send appboy user events ********************/

insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)
select user_id, attribute_name, data_type, promo_code as data_value, campaign_stream_steps_id, list_users_id from promo_code_users_sb where date_sent is null
except  select user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id  from appboy_user_events_sb where   campaign_stream_steps_id in (291,292,293) ;

update promo_code_users_sb
set date_sent = now()
where date_sent is null;

-- @wbtag C18_C11 success end date
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select t1.user_id, css2.attribute_name, css2.data_type, to_char(lu.list_start_date::date + day_end + 6 * INTERVAL '1 day', 'MM /DD /YYYY') as data_value, css2.id as campaign_stream_steps_id, t1.list_users_id  from (
select tss.user_id, tss.attribute_name, tss.data_type, tss.data_value, tss.campaign_stream_steps_id, tss.list_users_id, css.id, css.campaign_id from 
  (
  select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.list_users_id
    from tmp_sobe_challenge_13_success usw
    inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
    inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 1
    and css.day_in = usw.campaign_stream_steps_id
    where cast(usw.data_value as integer) >= 10
    union
    select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.list_users_id as list_users_id
    from tmp_sobe_challenge_13_success usw
    inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
    inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (6) and task_number = 2
    and css.day_in = usw.campaign_stream_steps_id
    where cast(usw.data_value as integer) >= 20
    union
    select usw.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, usw.list_users_id as list_users_id
    from tmp_sobe_challenge_13_success usw
    inner join list_users_c18_sobe lu on lu.user_id = usw.user_id and lu.deleted_at is null and lu.campaign_id = 1  and lu.deleted_at is null
    inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, '2018-09-17'::date)  between day_start and day_end and css.campaign_id in (6) and rule_type = 'result_status_success' and campaign_stream_sequence in (5) and task_number = 1
    and css.day_in = usw.campaign_stream_steps_id
    where cast(usw.data_value as integer) >= 200
    ) tss
inner join campaign_stream_steps_sobe css on tss.campaign_stream_steps_id = css.id) t1
inner join campaign_stream_steps_sobe css2 on t1.campaign_id = css2.campaign_id and css2.rule_type = 'promo_code_end_date'
inner join list_users_c18_sobe lu on lu.user_id = t1.user_id
except  select user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id  from appboy_user_events_sb where   campaign_stream_steps_id in (294) ;

/* C9 EXPIRATIONS *******************************************************************************************************************************/
/* @wbtag C18_C9_TIER_STATUS | BONUS_STATUS | EXPIRE_SUCCESS ******************************************************************************************/
insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select tt.user_id, tt.attribute_name, tt.data_type, tt.data_value, tt.campaign_stream_steps_id, tt.list_users_id from appboy_user_events_sb aue 
inner join (
select lu.user_id, css.attribute_name, css.data_type, css.data_value, css.id as campaign_stream_steps_id, lu.list_id as list_users_id, css.day_in
from list_users_c18_sobe lu 
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date)  between day_start and day_end and css.campaign_id = 2 and rule_type = 'result_status_success_expiration' -- and campaign_sequence = 1
where  lu.campaign_id = 1
) tt on aue.user_id = tt.user_id and aue.campaign_stream_steps_id = tt.day_in
except  select aue.user_id, aue.attribute_name, aue.data_type, aue.data_value, aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue;

drop table if exists tmp_c10_date_range_sobe;
drop table if exists tmp_success_users_sobe;
drop table if exists tmp_c10_date_range_sobe;
drop table if exists tmp_ns_challenge_10_day_sobe_aggregate;
drop table if exists tmp_success_summary; 
drop table if exists tmp_date_range_sobe_c12;
drop table if exists tmp_sobe_challenge_12;  
drop table if exists tmp_sobe_challenge_13_success;

update challenge_run_log
set updated_at = clock_timestamp()
where id in (select id from challenge_run_log where function_name = 'challenge_9_numi_sobe_update' order by created_at desc limit 1);

END;
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100;
ALTER FUNCTION public.challenge_super_sobe_update(timestamp without time zone, integer)
  OWNER TO gfuser;
