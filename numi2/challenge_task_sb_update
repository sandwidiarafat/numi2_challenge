from list_users_c18_sobe lu
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date) + 1 between css.day_start and css.day_end and css.campaign_id = 1 and rule_type = 'task_count' and task_number = 2 and campaign_sequence in (1,2,3,4,5)
	--and campaign_sequence = 1 remove campaign sequence 1/7/2018
except  select user_id, attribute_name, data_type, 'NOT COMPLETED' as data_value, campaign_stream_steps_id, list_users_id  from appboy_user_events_sb;


insert into appboy_user_events_sb (user_id, attribute_name, data_type, data_value, campaign_stream_steps_id, list_users_id)

select tt.user_id, tt.attribute_name, tt.data_type, '0' as data_value, tt.campaign_stream_steps_id, tt.list_users_id from (
select lu.user_id, css.attribute_name, css.data_type,  css.id as campaign_stream_steps_id, lu.list_id as list_users_id
from list_users_c18_sobe lu
inner join campaign_stream_steps_sobe css on datediff('day', lu.list_start_date::date, _datetime::date) + 1 between css.day_start and css.day_end and css.campaign_id = 1 and rule_type = 'task_count' and task_number = 2 and campaign_sequence in (6,7,8)
	-- and campaign_sequence = 1 removed campaign sequence 1/7/2018
where lu.campaign_id = 1
except  select aue.user_id, aue.attribute_name, aue.data_type,  aue.campaign_stream_steps_id, aue.list_users_id  from appboy_user_events_sb aue
inner join campaign_stream_steps_sobe css on aue.campaign_stream_steps_id = css.id and rule_type = 'task_count' and task_number = 2 and css.campaign_id = 1 and campaign_sequence in (6,7,8)) tt;



